{
  "name": "@fxtrot/edge",
  "description": "A component library for @foxtrot project",
  "version": "0.1.0",
  "author": "LexSwed <lexswed@gmail.com>",
  "license": "MIT",
  "module": "dist/edge.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch --transpileOnly",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --pattern \"src/**/*.*\" --staged && tsdx lint && tsc --noEmit"
    }
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.0",
    "@rollup/plugin-image": "^2.0.4",
    "@svgr/rollup": "^5.3.1",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/styled-components": "^5.1.0",
    "@types/styled-system": "^5.1.9",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-react-app": "^9.1.2",
    "cssnano": "^4.1.10",
    "docz": "^2.3.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-is": "^16.13.1",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "styled-components": "^5.1.1",
    "tsdx": "^0.13.2",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@popperjs/core": "^2.3.3",
    "@reach/auto-id": "^0.10.3",
    "@styled-system/should-forward-prop": "^5.1.5",
    "downshift": "^5.2.0",
    "styled-system": "^5.1.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "tarballDir": "dist"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": "dist/*.tgz"
        }
      ]
    ]
  }
}
